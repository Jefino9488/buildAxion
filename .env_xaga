# Default configuration for AxionOS xaga automation
# Copy to .env and edit as needed.

# Flow control
RUN_ENV_SETUP=true
RUN_SOURCE_SYNC=true
RUN_BUILD=false
DRY_RUN=false

# AxionOS/AOSP repo
AXION_REMOTE_URL=https://github.com/AxionAOSP/android.git
AXION_BRANCH=lineage-23.0

# Source sync / device
WORKDIR=axionos
THREADS=
WITH_MIUI_CAM=false
APPLY_WPA_PATCHES=false

# Build / Bot
DEVICE=xaga
VARIANT=userdebug
ROM_TYPE=axion-pico
CONFIG_OFFICIAL_FLAG=
UPLOAD_OTA_JSON=true
OTA_JSON_PATH=vendor/ota/your_device_name.json
PIN_SUCCESS_MESSAGE=true

# Telegram
CONFIG_CHATID=
CONFIG_BOT_TOKEN=
CONFIG_ERROR_CHATID=

# Uploads
RCLONE_REMOTE=
RCLONE_FOLDER=

# Power
POWEROFF=false
